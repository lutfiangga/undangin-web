import About from "../section/About.astro";
import Cta from "../section/Cta.astro";
import Faq from "../section/Faq.astro";
import Feature from "../section/Feature.astro";
import Pricing from "../section/Pricing.astro";
import Theme from "../section/Theme.astro";
import Value from "../section/Value.astro";

const message = "Halo, saya mau order undangan digital";
